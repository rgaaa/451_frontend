<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.INTERNET" />



    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"

        android:networkSecurityConfig="@xml/network_security_config"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.EECE451Project"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

<!--                <category android:name="android.intent.category.LAUNCHER" />-->
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.EECE451Project"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity" />
        <activity android:name=".MenuPatientActivity" />
        <activity android:name=".MenuPersonnelActivity" />
        <activity android:name=".MenuGeneralActivity" />
        <activity android:name=".BookingActivity" />
        <activity android:name=".UserInfoActivity" />
        <activity android:name=".ConfirmationActivity" />
        <activity android:name=".SearchPatientActivity" />
        <activity android:name=".SearchPersonnelActivity" />
        <activity android:name=".InfoAdminPersonnelActivity" />





    </application>

</manifest>